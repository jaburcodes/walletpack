    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.5.5",
    "@babel/runtime": "^7.0.0",
    "babel-loader": "^8.0.2",
    "babel-preset-minify": "^0.5.0-alpha.3cc09dcf",
    "chai": "^4.1.2",
    "concurrently": "^6.0.0",
    "lerna": "^3.15.0",
    "mocha": "^5.2.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0"
